    "ts-node-dev": "^1.1.1",
    "typeorm": "0.2.31",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.12",
    "@types/express": "^4.17.11",
    "@types/node": "^8.0.29",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "source-map-support": "^0.5.19",
    "sql.js": "^1.4.0",
    "ts-node": "3.3.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "tsc",
    "dev": "set debug=* && NODE_ENV=dev ts-node-dev --respawn --transpile-only ./src/index.ts",
    "prod": "NODE_ENV=production tsc && NODE_ENV=production node ./build/index.js",
    "start": "NODE_ENV=production node ./build/index.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:prod": "heroku run npx typeorm migration:run",
    "deploy": "git push heroku",
    "lint": "eslint --fix ./src/**/*.ts",
    "test": "jest",
    "reset": "rm -rf node_modules/ && npm install && npm run build"
  }
}
