    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/moment": "^2.13.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.1",
    "chai": "^4.3.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jest": "^26.6.3",
